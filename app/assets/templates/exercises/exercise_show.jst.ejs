Exercise Number:
<br>
<select class="exercise-list">
  <% exercises.each(function (exercise, idx) { %>
    <option
      data-id="<%= exercise.id %>"
      <% if (exercise === activeExercise) { %>
        selected
      <% } %>><%= idx + 1 %></option>
  <% }) %>
</select>
<br>
<h3>Readings</h3>
<%= activeExercise.get("readings") %>
<hr>
<h3>Instructions</h3>
<%= activeExercise.get("instructions") %>
